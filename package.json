{
  "name": "@n1ru4l/toposort",
  "version": "0.0.1",
  "description": "toposort using modern ecma script data structures",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "typings": "dist/typings/index.d.ts",
  "typescript": {
    "definition": "dist/typings/index.d.ts"
  },
  "author": {
    "name": "Laurin Quast",
    "email": "laurinquast@googlemail.com"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/typings/index.d.cts",
        "default": "./dist/cjs/index.js"
      },
      "default": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "types": "./main.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/n1ru4l/toposort"
  },
  "keywords": [
    "toposort",
    "dependency",
    "dag",
    "dependency list",
    "topological sort",
    "sort",
    "concurrency",
    "graph",
    "directed acyclic graph"
  ],
  "devDependencies": {
    "@changesets/cli": "2.27.9",
    "@types/jest": "26.0.23",
    "bob-the-bundler": "6.0.0",
    "cross-env": "7.0.3",
    "husky": "4.3.8",
    "jest": "26.6.3",
    "lint-staged": "10.5.4",
    "prettier": "2.3.2",
    "rimraf": "3.0.2",
    "ts-jest": "26.5.5",
    "tslib": "2.5.0",
    "typescript": "5.0.4"
  },
  "scripts": {
    "build": "bob build",
    "test": "jest",
    "type-check": "tsc",
    "release": "changeset publish",
    "format": "prettier .",
    "format:lint": "prettier --check ."
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{yml,js,ts,md,json}": "prettier --write"
  }
}
